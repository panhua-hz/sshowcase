<?xml version="1.0" encoding="UTF-8"?>
<configuration status="off">
	<appenders>
		<Console name="Console" target="SYSTEM_OUT">
			<PatternLayout
				pattern="%d{yyyy-MM-dd HH:mm:ss} %-5level [%thread][%class.%method:%line] - %msg%n" 
				charset="UTF-8"/>
		</Console>
	</appenders>
	<loggers>
		<root level="info">
			<AppenderRef ref="Console" />
		</root>
	</loggers>
</configuration> 